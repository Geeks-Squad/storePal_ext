{
  "manifest_version": 2,
  "name": "New Extension",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.min.js","content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "download.png",
    "default_popup": "popup.html"
  },
  "permissions": [
      "<all_urls>","webRequest","webRequestBlocking","tabs","http://127.0.0.1:8000/getdata","http://*/","https://*/","notifications"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
